{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "env": {
                "DREAMSIM_CACHE_DIR": "${workspaceFolder}/models",
                "DYLD_LIBRARY_PATH": "/opt/homebrew/lib:${env:DYLD_LIBRARY_PATH}"
            }
        },
        {
            "name": "Debug svg_eval.py --debug_dump --dataset svg-stack --client openai --model gpt-4.1-mini",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/svg_eval.py",
            "args": ["--debug_dump", "--dataset", "svg-stack", "--client", "openai", "--model", "gpt-4.1-mini", "-n", "300"],
            "console": "integratedTerminal",
            "env": {
                "DREAMSIM_CACHE_DIR": "${workspaceFolder}/models",
                "DYLD_LIBRARY_PATH": "/opt/homebrew/lib:${env:DYLD_LIBRARY_PATH}"
            }
        }
    ]
}